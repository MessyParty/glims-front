{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://dev.glims.store/login/:paths*"
    },
    {
      "source": "/:path*/",
      "destination": "https://dev.glims.store/api/v1/:path*",
      "has": [{ "type": "header", "key": "x-rewrite-target", "value": "true" }]
    }
  ]
}
